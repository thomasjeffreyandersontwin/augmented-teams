{
  "mcp": {
    "server_name": "{bot_name}",
    "command": "python",
    "args": [
      "agile_bot/bots/{bot_name}/src/{bot_name}_mcp_server.py"
    ],
    "cwd": "{workspace_path}",
    "env": {
      "AGENT_NAME": "{bot_name}",
      "PYTHONPATH": "{workspace_path}"
    }
  }
}





































































