{
  "domain": "Agile Bot Framework",
  "concepts": [
    {
      "concept": "Base Bot",
      "responsibilities": [
        {"responsibility": "Executes Actions", "collaborators": ["Workflow", "Behavior", "Action"]},
        {"responsibility": "Track activity", "collaborators": ["Behavior", "Action"]},
        {"responsibility": "Route to behaviors and actions", "collaborators": ["Router", "Trigger Words"]},
        {"responsibility": "Persist content", "collaborators": ["Content"]},
        {"responsibility": "Manage Project State", "collaborators": ["Project"]},
        {"responsibility": "Render", "collaborators": []}
      ],
      "note": "Actions are one of: InitializeProjectAction, GatherContextAction, PlanningAction, BuildKnowledgeAction, RenderOutputAction, ValidateRulesAction, CorrectBotAction"
    },
    {
      "concept": "Specific Bot",
      "responsibilities": [
        {"responsibility": "Provide Behavior config", "collaborators": ["Bot Config", "Behavior"]},
        {"responsibility": "Provide MCP config", "collaborators": ["MCP Config"]},
        {"responsibility": "Provide Renderers", "collaborators": []},
        {"responsibility": "Provide Extractors", "collaborators": []},
        {"responsibility": "Provide Synchronizer", "collaborators": []},
        {"responsibility": "Provide Trigger Words", "collaborators": []}
      ]
    },
    {
      "concept": "Behavior Workflow",
      "responsibilities": [
        {"responsibility": "Determine next Action", "collaborators": ["Behavior", "Action", "State"]},
        {"responsibility": "Track state", "collaborators": ["Behavior", "Action", "State"]}
      ]
    },
    {
      "concept": "Project",
      "responsibilities": [
        {"responsibility": "Move project to working area", "collaborators": ["Working Directory"]},
        {"responsibility": "Save project in context", "collaborators": ["Working Directory", "Workflow State"]},
        {"responsibility": "Update project area", "collaborators": ["Working Directory", "Content"]}
      ]
    },
    {
      "concept": "Behavior",
      "responsibilities": [
        {"responsibility": "Perform Configured Actions", "collaborators": ["Actions"]},
        {"responsibility": "Invoke On Trigger Words", "collaborators": ["List"]},
        {"responsibility": "Inject Instructions", "collaborators": ["Text"]},
        {"responsibility": "Provide Guardrails", "collaborators": ["GuardRails"]},
        {"responsibility": "Provide Rules", "collaborators": ["Rule", "Validation"]},
        {"responsibility": "Provide Content Specs", "collaborators": ["Content"]}
      ]
    },
    {
      "concept": "Base Action",
      "responsibilities": [
        {"responsibility": "Inject Instructions", "collaborators": ["Behavior"], "note": "must be overridden"},
        {"responsibility": "Load Relevant Content + Inject Into Instructions", "collaborators": ["Content"], "note": "must be overridden"},
        {"responsibility": "Save content changes", "collaborators": ["Content"], "note": "must be overridden"}
      ]
    },
    {
      "concept": "GatherContextAction",
      "action_number": 2,
      "responsibilities": [
        {"responsibility": "Inject gather context instructions", "collaborators": ["Behavior", "Guardrails", "Required Clarifications"]},
        {"responsibility": "Inject questions and evidence", "collaborators": ["Behavior", "Guardrails", "Key Questions", "Evidence"]}
      ]
    },
    {
      "concept": "PlanningAction",
      "action_number": 3,
      "responsibilities": [
        {"responsibility": "Inject planning instructions", "collaborators": ["Behavior", "Guardrails", "Planning"]},
        {"responsibility": "Inject decision criteria and assumptions", "collaborators": ["Behavior", "Guardrails", "Decision Criteria", "Assumptions", "Recommended Human Activity"]}
      ]
    },
    {
      "concept": "BuildKnowledgeAction",
      "action_number": 4,
      "responsibilities": [
        {"responsibility": "Inject knowledge graph template", "collaborators": ["Behavior", "Content", "Knowledge Graph Spec", "Knowledge Graph"]},
        {"responsibility": "Inject builder instructions", "collaborators": ["Behavior", "Content", "Build Instructions"]},
        {"responsibility": "Save Knowledge graph", "collaborators": ["Behavior", "Content", "Knowledge Graph"]}
      ]
    },
    {
      "concept": "RenderOutputAction",
      "action_number": 5,
      "responsibilities": [
        {"responsibility": "Inject render output instructions", "collaborators": ["Behavior", "Content", "Render Spec", "Renderer"]},
        {"responsibility": "Inject templates", "collaborators": ["Behavior", "Content", "Render Spec", "Template"]},
        {"responsibility": "Inject transformers", "collaborators": ["Behavior", "Content", "Transformer"]},
        {"responsibility": "Load + inject structured content", "collaborators": ["Behavior", "Content", "Knowledge Graph"]}
      ]
    },
    {
      "concept": "ValidateRulesAction",
      "action_number": 6,
      "responsibilities": [
        {"responsibility": "Inject common bot rules", "collaborators": ["Base Bot", "Rules", "Common Rules"]},
        {"responsibility": "Inject behavior specific rules", "collaborators": ["Behavior", "Rules", "Behavior Rules"]},
        {"responsibility": "Load + inject content for validation", "collaborators": ["Behavior", "Content", "Knowledge Graph", "Rendered Outputs"]}
      ]
    },
    {
      "concept": "CorrectBotAction",
      "action_number": 7,
      "responsibilities": [
        {"responsibility": "Inject correct bot instructions", "collaborators": ["Behavior", "Correct Bot Instructions"]},
        {"responsibility": "Load + inject diagnostics results", "collaborators": ["Content", "Diagnostic Report", "Violations", "Suggestions"]}
      ]
    },
    {
      "concept": "Guardrails",
      "responsibilities": [
        {"responsibility": "Provide required context", "collaborators": ["Key Questions", "Evidence"]},
        {"responsibility": "Guide planning decisions", "collaborators": ["Decision Criteria", "Assumptions"]},
        {"responsibility": "Define recommended human activity", "collaborators": ["Human", "Instructions"]}
      ]
    },
    {
      "concept": "Content",
      "responsibilities": [
        {"responsibility": "Render outputs", "collaborators": ["Template", "Renderer", "Render Spec"]},
        {"responsibility": "Synchronize formats", "collaborators": ["Synchronizer", "Extractor", "Synchronizer Spec"]},
        {"responsibility": "Save knowledge graph", "collaborators": ["Knowledge Graph"]},
        {"responsibility": "Load rendered content", "collaborators": ["na"]},
        {"responsibility": "Present rendered content", "collaborators": ["na"]}
      ]
    },
    {
      "concept": "Router",
      "responsibilities": [
        {"responsibility": "Match trigger patterns", "collaborators": ["Trigger Words", "Route"]},
        {"responsibility": "Route to MCP bot tool", "collaborators": ["Base Bot", "Trigger Words"]},
        {"responsibility": "Route to behavior tool", "collaborators": ["Behavior", "Trigger Words"]},
        {"responsibility": "Route to action tool", "collaborators": ["Action", "Trigger Words"]},
        {"responsibility": "Forward to behavior", "collaborators": ["Behavior", "Base Bot"]},
        {"responsibility": "Forward to action", "collaborators": ["Action", "Behavior"]},
        {"responsibility": "Forward to current behavior and action", "collaborators": ["Behavior", "Action", "Base Bot"]}
      ]
    },
    {
      "concept": "Template",
      "responsibilities": [
        {"responsibility": "Define output structure", "collaborators": ["Placeholder"]},
        {"responsibility": "Transform content", "collaborators": ["Transformer", "Content"]},
        {"responsibility": "Load template", "collaborators": ["Behavior", "Content"]}
      ]
    },
    {
      "concept": "Extractor / Synchronizer",
      "responsibilities": [
        {"responsibility": "Extract from source", "collaborators": ["Template", "Knowledge Graph"]},
        {"responsibility": "Load + inject rendered content", "collaborators": ["AI Chat", "Content"]}
      ]
    },
    {
      "concept": "Renderer",
      "responsibilities": [
        {"responsibility": "Render complex output", "collaborators": ["Template", "Knowledge Graph", "Transformer"]},
        {"responsibility": "Render outputs using components in context", "collaborators": ["AI Chat", "Template", "Content"]}
      ]
    },
    {
      "concept": "Rule",
      "responsibilities": [
        {"responsibility": "Validate content", "collaborators": ["Knowledge Graph", "Violations"]},
        {"responsibility": "Find behavior specific rules from context", "collaborators": ["Behavior"]},
        {"responsibility": "Find common bot rules from context", "collaborators": ["Base Bot"]},
        {"responsibility": "Load + inject diagnostics results", "collaborators": ["AI Chat", "Violations", "Corrections"]},
        {"responsibility": "Suggest corrections", "collaborators": ["Violations", "Suggestions", "Fixes"]},
        {"responsibility": "Provide examples - Do", "collaborators": ["Example", "Description"]},
        {"responsibility": "Provide examples - Dont", "collaborators": ["Example", "Description"]},
        {"responsibility": "Specialized examples", "collaborators": ["Language", "Framework", "Pattern"]}
      ]
    },
    {
      "concept": "Workflow State",
      "responsibilities": [
        {"responsibility": "Track current action", "collaborators": ["Action"]},
        {"responsibility": "Track completed actions", "collaborators": ["Action", "Activity Log"]},
        {"responsibility": "Determine next action", "collaborators": ["Action", "Behavior"]},
        {"responsibility": "Pause workflow", "collaborators": ["Human", "AI Chat"]},
        {"responsibility": "Resume workflow", "collaborators": ["Human", "AI Chat"]}
      ]
    }
  ]
}
